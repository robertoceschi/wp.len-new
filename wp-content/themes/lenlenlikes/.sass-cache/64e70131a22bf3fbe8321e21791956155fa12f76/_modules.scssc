// Main navigation links
// ====================================================================
body.active {
  nav[role='navigation'] {
    padding-bottom: 99999px !important;
    margin-bottom: -99999px !important;
    text-align: center;
    background-color: $color10;
    aside#main_nav {
      margin: 2.5em;
      overflow: hidden;
      list-style: none;
      a {
        display: block;
        border-bottom: none;
        color: $color11;
        opacity: .8;
        text-transform: uppercase;
        padding: .3em;
        font-size: 1.2em;

        &:hover {
          opacity: 1;
        }
        &:visited {
          opacity: .9;
        }
      }
    }
    // social links
    // ======================================================================
    aside#sociallinks {
      @include squish(1, 1, 9);
      margin-top: 5em;
      margin-bottom: 5em;
      ul.sociallinks {
        //@extend ul.chapter-list;
        li {
          text-align: center;
          a {
            border-bottom: none;
            display: inline;
            margin: 0 5px 0 5px;

            .icon-facebook {
              font-size: 2.1em;
              color: $color11;
              opacity: .8;
              &:hover {
                opacity: 1;
              }
              &:visited {
                opacity: .9;
              }
            }
            .icon-twitter {
              @extend .icon-facebook;
            }
            .icon-instagram {
              @extend .icon-facebook;
            }

          }
        }

      }
      h3 {
        position: relative;
        margin-top: 20px;
        color: $color11;
      }
      h3.one {
        margin-top: 0;
      }
      h3.one:before {
        content: "";
        display: block;
        border-top: solid 1px $color11;
        width: 100%;
        height: 1px;
        position: absolute;
        top: 50%;
        z-index: 1;
      }
      h3.one span {
        background: black;
        padding: 0 20px;
        position: relative;
        z-index: 5;
      }
    }
    // about me
    // ======================================================================
    aside#about_me {
      @extend #sociallinks;

      .about-widget-container {

        @include at-breakpoint($M) {
        max-width: 650px;
        margin: 0 auto;
        }
        .about-img-wrap {
          text-align: center;

          margin: 6px 4% 0 0;
          p {
            color: $color12;
            line-height: 1.7;
            letter-spacing: 1px;
            font-size: 0.8em;
            text-align: center;
            font-style: italic;
            text-transform: uppercase;
            @include at-breakpoint($M) {

               }
          }
          @include at-breakpoint($L) {
          }
        }
        .about-text-wrap {
          p {
            color: $color12;
            text-align: left;
            line-height: 1.5;
            font-size: 0.85em;
            @include at-breakpoint($M) {
              font-size: 0.98em;
              }

          }
          @include at-breakpoint($L) {
          }
        }
      }
    }
    // Search
    // ====================================================================
    aside#search {
      @extend #sociallinks;
      //@extend ul.chapter-list;
    }
    /* Form wrapper styling */
    .form-wrapper {
      width: 200px;
      @include at-breakpoint($S) {
        width: 350px;
      }
      @include at-breakpoint($M) {
        width: 450px;
      }
      padding: 0 15px 0 15px;
      margin: 0 auto 130px auto;
      background: #444;
      background: rgba(0, 0, 0, .2);
      border-radius: 10px;

    }

    /* Form text input */

    .form-wrapper input {
      font-family: $body_font;
      width: 100px;
      @include at-breakpoint($S) {
        width: 210px;
        font-size: 15px;
      }
      @include at-breakpoint($M) {
        width: 310px;
        font-size: 15px;
      }
      height: 40px;
      padding: 10px 10px;
      float: left;
      border: 0;
      background: #eee;
      font-size: 14px;
      letter-spacing: 1px;
    }

    .form-wrapper input:focus {
      outline: 0;
      background: #fff;
    }

    .form-wrapper input::-webkit-input-placeholder {
      color: #999;
      font-weight: normal;
      font-style: italic;
    }

    .form-wrapper input:-moz-placeholder {
      color: #999;
      font-weight: normal;
      font-style: italic;
    }

    .form-wrapper input:-ms-input-placeholder {
      color: #999;
      font-weight: normal;
      font-style: italic;
    }

    /* Form submit button */
    .form-wrapper button {
      font-family: proxima-nova;
      overflow: visible;
      position: relative;
      float: right;
      border: 0;
      padding: 0;
      cursor: pointer;
      height: 40px;
      width: 70px;
      @include at-breakpoint($S) {
        width: 110px;
        font-size: 14px;
      }
      @include at-breakpoint($M) {
        font-size: 16px;
      }
      font-size: 13px;
      color: #fff;
      text-transform: uppercase;
      background: $color13;
      text-shadow: 0 -1px 0 rgba(0, 0, 0, .3);
      letter-spacing: 1px;
    }

    .form-wrapper button:hover {
      background: lighten($color13, 7%);

    }

    .form-wrapper button:active,
    .form-wrapper button:focus {
      background: $color13;
      outline: 0;
    }

    .form-wrapper button:before {
      /* left arrow */
      content: '';
      position: absolute;
      border-width: 8px 8px 8px 0;
      border-style: solid solid solid none;
      border-color: transparent $color13 transparent;
      top: 12px;
      left: -6px;
    }

    .form-wrapper button:hover:before {
      border-right-color: lighten($color13, 7%);
    }

    .form-wrapper button:focus:before,
    .form-wrapper button:active:before {
      border-right-color: $color13;
    }

    .form-wrapper button::-moz-focus-inner {
      /* remove extra button spacing for Mozilla Firefox */
      border: 0;
      padding: 0;
    }

    aside#categories {
      @extend #sociallinks;
      ul {
        text-align: center;
        li.cat-item {
          display: inline-block;
          border: 1px solid $color11;
          text-transform: uppercase;
          letter-spacing: 1px;
          margin: 3%;
          @include at-breakpoint($M) {
            margin: 1.5%;
          }
          padding: 3%;
          @include at-breakpoint($S) {
            padding: 1%;
          }
          &:hover {
            background-color: lighten($color13, 7%);
            border: 1px solid lighten($color13, 7%);
          }
          a {
            color: $color11;
            border-bottom: none;
            //padding: 4%;
          }
        }
      }
    }
  }
}

