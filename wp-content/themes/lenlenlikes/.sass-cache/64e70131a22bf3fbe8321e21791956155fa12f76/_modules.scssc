header[role='banner'] {
  width: 100%;
  height: 50px;
  overflow: hidden;
  a {
    border-bottom: none;
    color: black;
  }
  @include at-breakpoint($L) {
    position: fixed;
    z-index: 100000 !important;
    background: rgba(255, 255, 255, 0.7);
  }
  span.sidebar-item {
    min-width: 65px;
    display: block;
    float: left;
    margin-left: 2.125%;

    .sidebar_button {
      display: block;
      height: 50px;
      padding: 21px 45px 0 0;
      @include at-breakpoint($L) {
        padding-top: 19px;
      }
      font-size: 0.8em;
      @include at-breakpoint($L) {
        font-size: 1em;
      }
      color: rgb(0, 0, 0);
      text-align: right;
      font-family: $header_font;
      span.menu_title {
        padding-left: 15%;
      }
    }
  }
  div.site-title {
    width: 68%;
    min-height: 50px;
    margin-left: 31%;
    padding-top: 17px;
    z-index: 100000;
    h1.main-link-headline {
      margin: 2% 2% 0 0;
      padding-bottom: 5px;
      font-family: $header_font;
      font-size: 1em;
      font-weight: normal;
      line-height: 1.3;
      word-wrap: break-word;
      letter-spacing: 1px;
      display: block;
      text-align: right;
      @include at-breakpoint($L) {
        font-size: 1.3em;
        position: fixed;
        top: 0;
        right: 0;
        margin-right: 3%;
        margin-top: 1.1%;
      }
    }
  }
}

